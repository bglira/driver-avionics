ccflags-y 		:= -D__CHECK_ENDIAN__ -DDEBUG

# ARINC-429 Device Drivers (would be in drivers/net/arinc429 in the kernel)
obj-m			+= varinc429.o

obj-m			+= arinc429-dev.o
arinc429-dev-y	:= dev.o

# ARINC-429 Network interfaces (would be in net/arinc429 in the kernel)
obj-m			+= arinc429.o
arinc429-y		:= af_arinc429.o proc.o

obj-m			+= arinc429-raw.o
arinc429-raw-y	:= raw.o

